{
  "name": "Urbindex",
  "short_name": "Urbindex",
  "description": "Community-driven urban exploration mapping tool",
  "start_url": "/final.html",
  "id": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone", "browser"],
  "background_color": "#080818",
  "theme_color": "#00e5ff",
  "orientation": "any",
  "icons": [],
  "splash_pages": null,
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "shortcuts": [],
  "categories": ["maps", "social", "lifestyle", "navigation", "travel"],
  "screenshots": [],
  "related_applications": [],
  "prefer_related_applications": false,
  "iarc_rating_id": "",
  "share_target": {
    "action": "/final.html?share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "capture_links": "existing_client_event",
  "protocol_handlers": [
    {
      "protocol": "web+urbindex",
      "url": "/final.html?protocol=%s"
    }
  ],
  "note_taking": {
    "new_note_url": "/final.html?new-note=true"
  },
  "file_handlers": [
    {
      "action": "/final.html?import",
      "accept": {
        "application/gpx+xml": [".gpx"],
        "application/geo+json": [".geojson"]
      }
    }
  ],
  "permissions": ["geolocation", "camera", "notifications"]
}
