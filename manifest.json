{
  "name": "Urbindex",
  "short_name": "Urbindex",
  "description": "Community-driven urban exploration mapping tool",
  "start_url": "/index.html",
  "id": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "minimal-ui", "standalone", "browser"],
  "background_color": "#080818",
  "theme_color": "#00e5ff",
  "orientation": "any",
  "icons": [
    {
      "src": "images/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "images/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "images/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "images/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "splash_pages": null,
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "shortcuts": [
    {
      "name": "Add Location",
      "short_name": "Add",
      "description": "Quickly add a new exploration location",
      "url": "/index.html?action=add",
      "icons": [
        {
          "src": "images/icons/icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Map",
      "short_name": "Map",
      "description": "Open the exploration map",
      "url": "/index.html?view=map",
      "icons": [
        {
          "src": "images/icons/icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Locations",
      "short_name": "Locations",
      "description": "View your exploration locations",
      "url": "/index.html?view=locations",
      "icons": [
        {
          "src": "images/icons/icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "categories": ["maps", "social", "lifestyle", "navigation", "travel"],
  "screenshots": [],
  "related_applications": [],
  "prefer_related_applications": false,
  "iarc_rating_id": "",
  "share_target": {
    "action": "/index.html?share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "capture_links": "existing_client_event",
  "protocol_handlers": [
    {
      "protocol": "web+urbindex",
            "url": "/index.html?protocol=%s"
    }
  ],
  "note_taking": {
    "new_note_url": "/index.html?new-note=true"
  },
  "file_handlers": [
    {
      "action": "/index.html?import",
      "accept": {
        "application/gpx+xml": [".gpx"],
        "application/geo+json": [".geojson"]
      }
    }
  ],
  "permissions": ["geolocation", "camera", "notifications"],
  "start_url": "/index.html"
}
